URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,matcher,font-size
!URxvt*font: xft:Consolas:size=16, style=regular
!URxvt.boldFont: xft:Consolas:size=16, style=bold
URxvt.letterSpace:-0.5
URxvt*internalBorder: 14
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt.url-launcher: usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.C-equal: perl:font-size:increase
URxvt.keysym.C-minus: perl:font-size:decrease
URxvt*depth: 32
URxvt*background: [40]black
URxvt*font:xft:Hack:size=16


